public class Vocalist extends Performer {
	private String key;
	
	public void perform () {
		System.out.println("I sing in the key of - " + key + " - " + id);
	}
	
	public void perform (int volume) {
		System.out.println("I sing in the key of - " + key + " - at the volume " + volume + " - " + id);
	}
}
